				<%= f.text_field :company_name %><br /><br />
				<%= f.text_field :tagline %><br />
				<br />
				<div class="textarea">
					<%= f.text_area :address_line_1 %><br />
					<br />
					<%= f.text_area :address_line_2 %><br />
