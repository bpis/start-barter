<article id="content">
  <section class="col1">

    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => {:id => "newsletter"}) do |f| %>
  
      <%= devise_error_messages! %>

      <h4>Email</h4><br />
      <div class="bg">
        <%= f.email_field :email, :placeholder => "Type Your Email Here", :class => "input" %>
      </div> 

    	<h4>First Name</h4><br />
      <div class="bg">  
        <%= f.text_field :first_name, :placeholder => "Type Your First Name Here", :class => "input" %>
      </div>

    	<h4>Last Name</h4><br />
      <div class="bg">
        <%= f.text_field :last_name, :placeholder => "Type Your Last Name Here", :class => "input" %>
      </div>

      <h4>Username</h4><br />
      <div class="bg">
        <%= f.text_field :username, :placeholder => "Type Your Username Here", :class => "input" %>
      </div>
      
      <h4>Password</h4><span class="hint">(minimum 8 characters required)</span><br />
      <div class="bg">
        <%= f.password_field :password, :placeholder => "Type Your Password Here", :class => "input" %>
      </div>

      <h4>Retype Password</h4><br />
      <div class="bg"> 
        <%= f.password_field :password_confirmation, :placeholder => "Retype your Password Here", :class => "input" %>
      </div>

    <h4>I am a</h4><span class="hint">(select multiple skills)</span>
      <%= f.select :iam, options_for_select(["Admin Support", "Designer", "Developer", "Content Writer", "Marketer", "Multimedia Expert", "Web developer", "Legal Expert", "Translator", "Sales Professional", "Finance Professional", "Consultant", "Engineer", "Manufacturing Professional"]), {}, {:multiple => true} %><br/>
      <h4>I am looking for </h4><span class="hint">(select multiple skills)</span>
        <%= f.select :iamlookingfor, options_for_select(["Admin Support", "Designer", "Developer", "Programmers", "Content Writer", "Marketer", "Multimedia Experts", "Web developers", "Legal Experts", "Translators", "Sales Professionals", "Finance Professionals", "Consultants", "Engineers", "Manufacturing Professionals"]), {}, {:multiple => true} %><br/>

      <h4>Country</h4><br/>
      <div >
        <%= f.collection_select(:country_id, Country.all, :id, :name ) %>
      </div><br />
		<h4> Profile Picture </h4>
		<div><%= f.file_field :profile_picture %></div><br />
      <div><%= f.submit "Sign up", :class=> "button" %></div>
    <% end %>


    <%#= render "devise/shared/links" %>
    <%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
      <div><%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name) %></div>
    <% end -%>
    <div class="wrapper pad_bot2">
      <figure class="left marg_right1"><a href="#">
                <p class="pad_bot1 pad_top1"><span class="color1"></span></p>
      <p></p>
    </div>
    <div class="wrapper pad_bot2">
      <figure class="left marg_right1"><a href="#"></a></figure>
      <p class="pad_bot1 pad_top1"><span class="color1"></span></p>
      <p> </p>
    </div>
    <div class="wrapper">
      <figure class="left marg_right1"><a href="#"></a></figure>
      <p class="pad_bot1 pad_top1"><span class="color1"></span></p>
      <p></p>
    </div>
  </section>
